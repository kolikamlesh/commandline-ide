rcol() {
    bstcol=( 1 2 3 5 6 11 13 14 15 21 33 51 57 63 64 68 100 106 111 123 128 130 131  134 136
              140 142 141 143 147 148 153 154 159 165 166 189 190 206 211 214 218 220 226
               255 )
               
        num=$(($RANDOM%45))
     # returning desire color
     col ${bstcol[num]}
   
   }

   rcol
